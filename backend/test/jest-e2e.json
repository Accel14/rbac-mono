{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@users/(.*)$": "<rootDir>/src/api/users/1",
    "^@entities/(.*)$": "<rootDir>/src/entities/$1",
    "^@enums/(.*)$": "<rootDir>/src/enums/$1"
  }
}